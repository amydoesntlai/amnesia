<!DOCTYPE html>
<html>
<head>
  <title>Vocabulary</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
	<%= current_user.username %> <%= link_to '(logout)', log_out_path %>
<% else %>
	<%= link_to 'Login', sessions_path %>
<% end %>
<%= yield %>

</body>
</html>
